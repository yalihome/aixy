{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": ["error", {
            "typeof": false
        }],
        "no-unreachable": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "strict": 0
    },
    "globals": {
        "__ROUTER_MAP__": true,
        "__inline": true,
        "__uri": true,
        "__APP_DATA": true,
        "SCRATCONFIG": true,
        "__ROUTE_MAP__": true,
        "define": true,
        "WeixinJSBridge": true,
        "wx": true,
        "jQuery": true,
        "WeiboJS": true,
        "Ali": true,
        "Bridge": true,
        "PI_COLLECT": true,
        "__THEME__": true
    },
    "parser": "babel-eslint"
}